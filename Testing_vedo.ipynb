   "source": [
    "\"\"\"Align 2 shapes: a simple line to a polygonal mesh\"\"\"\n",
    "from vedo import *\n",
    "\n",
    "settings.default_backend = 'vtk' # or k3d, ipyvtk,trame or vtk\n",
    "\n",
    "limb = Mesh(\"panther.stl\")\n",
    "\n",
    "plt = Plotter()\n",
    "plt += [limb]\n",
    "plt.show(\"OpenShadows\",axes=1, viewup=\"z\", bg='bb')"
   ]
